{
  "name": "my-app-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "antd": "^4.11.2",
    "axios": "^0.19.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "mobx": "^5.10.1",
    "mobx-react": "^5.4.4",
    "mobx-react-devtools": "^6.1.1",
    "mobx-react-lite": "1.4.0",
    "mobx-react-router": "^4.0.7",
    "moment": "^2.27.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-transition-group": "^4.4.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "test": "react-scripts test",
    "lint-styl": "stylint src/styles",
    "start-ts": "react-app-rewired start --scripts-version react-scripts",
    "start": "react-app-rewired start",
    "build-ts": "set GENERATE_SOURCEMAP=true && react-app-rewired build --scripts-version react-scripts",
    "build": "npm-run-all build-ts",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint_quiet": "eslint --quiet 'src/**/*.{ts,tsx,js,jsx}'",
    "lint_fix": "eslint  --fix 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier": "prettier --write src/",
    "eject": "react-scripts eject",
    "commit": "git-cz",
    "start-debugger": "node --inspect-brk ./node_modules/react-app-rewired/scripts/start.js",
    "build-debugger": "node --inspect-brk ./node_modules/react-app-rewired/scripts/build.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/js-cookie": "^2.2.2",
    "@types/lodash": "^4.14.135",
    "@types/react-loadable": "^5.5.1",
    "@types/react-router": "^5.0.2",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-transition-group": "^2.9.2",
    "@types/webpack-env": "^1.13.9",
    "commitizen": "^4.2.3",
    "customize-cra": "^1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-alloy": "^4.3.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "react-app-rewired": "^2.1.8",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^4.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint-styl",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,css,ts,jsx,tsx}": [
      "prettier --write src/",
      "eslint --cache --fix 'src/**/*.{ts,tsx,js,jsx}'",
      "git add"
    ]
  },
  "config-overrides-path": "./webpack.config.js",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
