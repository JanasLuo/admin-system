{
  "name": "dp-antd",
  "version": "0.0.5",
  "description": "React components library",
  "author": "janas luo",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React"
  ],
  "repository": {
    "type": "git",
    "url": "https://git.haizhi.com/delivery/digital-police/dp-antd"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.11",
    "react-transition-group": "^4.4.1"
  },
  "scripts": {
    "start-ts": "react-app-rewired start --scripts-version react-scripts",
    "start": "react-app-rewired start",
    "clean": "rimraf ./dist",
    "build-js": "react-app-rewired build --scripts-version react-scripts",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "stylus src/styles/index.styl -o dist/index.css",
    "build": "npm run clean && npm run build-ts && npm run build-css",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true react-scripts test",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint_quiet": "eslint --quiet 'src/**/*.{ts,tsx,js,jsx}'",
    "lint_fix": "eslint --cache --fix 'src/**/*.{ts,tsx,js,jsx}'",
    "lint-styl": "stylint src/styles",
    "prettier": "prettier --write src/",
    "eject": "react-scripts eject",
    "commit": "git-cz",
    "start-debugger": "node --inspect-brk ./node_modules/react-app-rewired/scripts/start.js",
    "build-debugger": "node --inspect-brk ./node_modules/react-app-rewired/scripts/build.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "storybook-docs": "start-storybook --docs --no-manager-cache",
    "build-storybook-docs": "build-storybook --docs",
    "stylus": "stylus src/styles/index.styl -o src/styles/index.css",
    "prepublishOnly": "npm run test:nowatch && npm run lint && npm run build"
  },
  "peerDependencies": {
    "antd": "<=3.26.20",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-storyshots": "^6.3.10",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.135",
    "@types/node": "^12.0.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-transition-group": "^2.9.2",
    "@types/webpack-env": "^1.13.9",
    "@types/react-loadable": "^5.5.1",
    "@types/react-router": "^5.0.2",
    "@types/react-router-config": "^5.0.0",
    "@types/react-router-dom": "^5.1.7",
    "antd": "3.26.20",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-alloy": "^4.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react": "^17.0.1",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.1",
    "react-loadable": "^5.5.0",
    "react-router": "^5.2.0",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-test-renderer": "^17.0.2",
    "storybook-css-modules-preset": "^1.1.1",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "typescript": "^4.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^4.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty && yarn lint-styl",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "yarn prettier",
      "yarn lint_fix"
    ]
  },
  "config-overrides-path": "./webpack.config.js",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
